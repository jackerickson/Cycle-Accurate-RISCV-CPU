#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55628cd66740 .scope module, "top" "top" 2 11;
 .timescale 0 0;
v0x55628cd86d30_0 .var "x", 3 0;
v0x55628cd86e20_0 .net "z", 3 0, L_0x55628cd86f20;  1 drivers
E_0x55628cd620d0 .event edge, v0x55628cd86b30_0, v0x55628cd86c10_0;
S_0x55628cd668c0 .scope module, "c" "counter" 2 27, 2 1 0, S_0x55628cd66740;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "x"
    .port_info 1 /OUTPUT 4 "z"
L_0x7fadb252c018 .functor BUFT 1, C4<0001>, C4<0>, C4<0>, C4<0>;
v0x55628cd66a90_0 .net/2u *"_s0", 3 0, L_0x7fadb252c018;  1 drivers
v0x55628cd86b30_0 .net "x", 3 0, v0x55628cd86d30_0;  1 drivers
v0x55628cd86c10_0 .net "z", 3 0, L_0x55628cd86f20;  alias, 1 drivers
L_0x55628cd86f20 .arith/sum 4, v0x55628cd86d30_0, L_0x7fadb252c018;
    .scope S_0x55628cd66740;
T_0 ;
    %vpi_call 2 16 "$dumpfile", "hello-world.vcd" {0 0 0};
    %vpi_call 2 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55628cd668c0 {0 0 0};
    %delay 0, 0;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x55628cd86d30_0, 0, 4;
    %delay 10, 0;
    %load/vec4 v0x55628cd86d30_0;
    %addi 1, 0, 4;
    %store/vec4 v0x55628cd86d30_0, 0, 4;
    %delay 10, 0;
    %load/vec4 v0x55628cd86d30_0;
    %addi 1, 0, 4;
    %store/vec4 v0x55628cd86d30_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x55628cd66740;
T_1 ;
    %wait E_0x55628cd620d0;
    %vpi_call 2 25 "$display", "[Hello world] x:%h, z:%h", v0x55628cd86d30_0, v0x55628cd86e20_0 {0 0 0};
    %jmp T_1;
    .thread T_1, $push;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "hello-world.v";
